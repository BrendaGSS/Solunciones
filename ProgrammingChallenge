# include <bits/stdc++.h> //INCOMPLETO
using namespace std;
string post;
stack <char> p;

bool op(char c){
	if(c=='+' || c=='-' || c=='*' || c=='/'){return true;}
	return false;
}
int prioridad(char c){
	if (c=='+' || c=='-'){return 1;}
	if (c=='*' || c=='/'){return 2;}
}

void postfija(string s){
	for (int i=0;i<s.size();i++){
		if (op(s[i])){ // si es operador
			if(p.empty()){ //si la pila está vacía
				p.push(s[i]);
			}
			else{//no esta vacia

					int p1=prioridad(s[i]);
					int p2=prioridad(p.top());
					if (p1<=p2){
						while (!p.empty() && p1<=p2 ){
						post=post + p.top();
						p.pop();
						p2=prioridad(p.top());
						
					}
				}else{
						p.push(s[i]);
					
			}	
		}else //no es operador
			post=post + s[i];		
	}
}
string s;
int main (){

	cin>>s;
	postfija(s);
	for(int i=0;i<post.size();i++)
		cout<<post[i];	
	
}
